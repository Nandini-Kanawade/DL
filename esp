//File pref paste
//https://raw.githubusercontent.com/espressif/arduino-esp32/gh-pages/package_esp32_index.json
//Tools board manager esp32, 
//sketch dht11 adafruitunified

#include <DHT.h>
#define DHT_PIN 14 // Pin where the DHT11 is connected
#define LM35_PIN 34 // Pin where the LM35 is connected

DHT dht(DHT_PIN, DHT11); // Initialize DHT sensor


float temperatureDHT; // Variable to store temperature from DHT11


void setup() {
Serial.begin(115200);
dht.begin();
}


void loop() {
// Read temperature from DHT11
temperatureDHT = dht.readTemperature();
// Read voltage from LM35
int sensorValue = analogRead(LM35_PIN);
// Convert the analog reading to voltage
float voltage = sensorValue * (5(Vref) / 4095.0(Resolution));
// Convert the voltage to temperature in Celsius (LM35 has 10mV/°C scale)
float temperatureLM35 = voltage * 100.0;
// Print the results
Serial.print("Temperature (DHT11): ");
Serial.print(temperatureDHT);


Serial.println(" °C");


Serial.print("Temperature (LM35): ");


Serial.print(temperatureLM35);


Serial.println(" °C");


delay(2000); // Adjust the delay according to your needs
}

// Select board select comport
//Set baud rate

